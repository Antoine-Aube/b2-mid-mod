<h1>Departments<h1>
<% @departments.each do |department| %>
<section id="department-<%=department.id%>">
  <h2>Department: <%= department.name %></h2>
  <p>floor: <%= department.floor %></p>
    <% department.employees.each do  |employee| %>
      <p>Employee: <%= employee.name %></p>
    <% end %>
</section>
<% end %>

